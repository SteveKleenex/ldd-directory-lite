        <article id="listing-{{id}}" class="listing-{{id}} type-listing search-result {{nth}} cf">
            {{featured}}

            <div class="search-content">
                <header class="listing-header">
                    <h2 class="listing-title">{{title}}</h2>
                    <h3 class="listing-url"><a href="{{url}}">{{url}}</a></h3>
                </header><!-- .entry-header -->

                <div class="listing-summary">
                    {{summary}}
                </div><!-- .entry-summary -->
            </div>
        </article><!-- #listing-## -->

